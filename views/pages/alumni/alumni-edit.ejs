    <script>
        function toggleKegiatan(activeId) {
            const kegiatanIds = ['belumAdaKegiatan', 'bekerja', 'melanjutkanStudi', 'berwirausaha', 'kursus'];

            kegiatanIds.forEach(id => {
                const kegiatan = document.getElementById(id);
                console.log(kegiatan);
                const inputs = kegiatan.querySelectorAll('input', 'select', 'textarea');

                if (id === activeId) {
                    kegiatan.classList.remove('hidden');
                    kegiatan.classList.add('grid');
                    inputs.forEach(input => input.setAttribute('required', true));
                } else {
                    kegiatan.classList.add('hidden');
                    kegiatan.classList.remove('grid');
                    inputs.forEach(input => input.removeAttribute('required'));
                }
            });
        }

        const kegiatanLulus = document.querySelector('.kegiatanLulus');
        const containerKegiatan = document.getElementById('containerKegiatan');
        console.log(containerKegiatan)

        function toCamelCase(str) {
            return str
                .toLowerCase() // Ubah semua huruf menjadi huruf kecil
                .split(' ')    // Pecah string berdasarkan spasi
                .map((word, index) =>
                    index === 0
                        ? word // Kata pertama tetap huruf kecil
                        : word.charAt(0).toUpperCase() + word.slice(1) // Kata berikutnya huruf pertama besar
                )
                .join(''); // Gabungkan kembali tanpa spasi
        }

        kegiatanLulus.addEventListener('change', function () {
            containerKegiatan.classList.remove('hidden');
            toggleKegiatan(toCamelCase(kegiatanLulus.value));
        });
    </script>
